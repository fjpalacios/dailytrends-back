  "homepage": "https://github.com/fjpalacios/dailytrends-back#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.5",
    "node-cron": "^2.0.3",
    "puppeteer": "^1.12.2",
    "rss-parser": "^3.7.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
